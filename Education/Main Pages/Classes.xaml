        <Label x:Name="programtime" Content="Okres" HorizontalAlignment="Left" Margin="925,19,0,0" VerticalAlignment="Top" Width="109" Foreground="White" VerticalContentAlignment="Center" FontSize="14" Height="24" HorizontalContentAlignment="Center" FontFamily="Tw Cen MT"/>
        <Button x:Name="ADDProg" FontSize="10" Content="+ Dodaj program" Visibility="Visible" HorizontalAlignment="Left" Margin="396,19,0,0" VerticalAlignment="Top" Width="101" Background="{x:Null}" BorderBrush="{x:Null}"  Height="21" Style="{DynamicResource CHBut}" Click="ADDProg_Click" />
        <Button x:Name="ADDProg_Copy" FontSize="10" Content="- Usuń program" Visibility="Visible" HorizontalAlignment="Left" Margin="502,19,0,0" VerticalAlignment="Top" Width="95" Background="{x:Null}" BorderBrush="{x:Null}"  Height="21" Style="{DynamicResource CHBut}" Foreground="#FFEA2027"/>
        <Button x:Name="ADDProg_Copy1" FontSize="10" Content="+ Dodaj okres" Visibility="Visible" HorizontalAlignment="Left" Margin="1037,19,0,0" VerticalAlignment="Top" Width="95" Background="{x:Null}" BorderBrush="{x:Null}"  Height="21" Style="{DynamicResource CHBut}"/>
        <Rectangle Fill="#05c46b" HorizontalAlignment="Left" Height="40" Margin="43,105,0,0" VerticalAlignment="Top" Width="302"/>
        <Label x:Name="ClassesLbl" Content="Przedmioty objęte programem" HorizontalAlignment="Left" Margin="43,105,0,0" VerticalAlignment="Top" Width="302" Foreground="White" VerticalContentAlignment="Center" FontSize="22" Height="40" HorizontalContentAlignment="Center" FontFamily="Tw Cen MT"/>
        <Grid Background="#FFF5F6FA" HorizontalAlignment="Left" Height="424" Margin="43,145,0,0" VerticalAlignment="Top" Width="1118"/>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="424" Margin="43,145,0,0" VerticalAlignment="Top" Width="1118" BorderBrush="#FF05C46B">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Page>
